<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %} | Green Ambassadrice{% endblock %}</title>
        
        {% block stylesheets %} 
            {% stylesheets output="css/compiled/mains.css" filter="cssrewrite"
                "css/bootstrap.min.css"
                "css/jumbotron-narrow.css"
                "css/myStyle.css"
            %} 
                <link href="{{ asset_url }}" rel="stylesheet">
            {% endstylesheets %}

        
        {% endblock %}
         
        
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon1.ico') }}" />
        
    </head>
    <body>
        <div class="container">
            {% include 'HelloBundle:public:header.html.twig' %}
            {% block body %}

            <div class="jumbotron">
                {% block jumbotron %}
                {% endblock %}
            </div>
            <div class="row marketing">
                    <div class="col-lg-12">
                        {% block rowMarketing %}{% endblock %}
                    </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                {% block row %}{% endblock %}    
                </div>
            </div>



            {% endblock %}
            {% block javascripts %}{% endblock %}
            {% include 'HelloBundle:public:footer.html.twig' %}
        </div>
    </body>
</html>
